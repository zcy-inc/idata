<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="idata-develop" targetRuntime="MyBatis3DynamicSql">
        <commentGenerator>
            <property name="suppressDate" value="true" />
            <property name="addRemarkComments" value="true" />
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://mysql-5-7:30106/idata"
                        userId="root" password="123456"/>

        <javaModelGenerator targetPackage="cn.zhengcaiyun.idata.develop.dal.model" targetProject="src/main/java"/>
        <javaClientGenerator targetPackage="cn.zhengcaiyun.idata.develop.dal.dao" targetProject="src/main/java"/>

        <table tableName="dev_folder" mapperName="DevFolderDao">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
            <columnOverride column="del" javaType="java.lang.Integer"/>
        </table>
        <table tableName="dev_table_info" mapperName="DevTableInfoDao">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
            <columnOverride column="del" javaType="java.lang.Integer"/>
        </table>
        <table tableName="dev_column_info" mapperName="DevColumnInfoDao">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
            <columnOverride column="del" javaType="java.lang.Integer"/>
        </table>
        <table tableName="dev_label_define" mapperName="DevLabelDefineDao">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
            <columnOverride column="del" javaType="java.lang.Integer"/>
            <columnOverride column="label_required" javaType="java.lang.Integer"/>
            <columnOverride column="label_attributes" javaType="java.util.List"
                            typeHandler="cn.zhengcaiyun.idata.develop.dal.JsonColumnHandler"/>
            <columnOverride column="special_attribute" javaType="cn.zhengcaiyun.idata.develop.dto.label.SpecialAttributeDto"
                            typeHandler="cn.zhengcaiyun.idata.develop.dal.JsonColumnHandler"/>
        </table>
        <table tableName="dev_label" mapperName="DevLabelDao">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
            <columnOverride column="del" javaType="java.lang.Integer"/>
        </table>
        <table tableName="dev_enum" mapperName="DevEnumDao">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
            <columnOverride column="del" javaType="java.lang.Integer"/>
        </table>
        <table tableName="dev_enum_value" mapperName="DevEnumValueDao">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
            <columnOverride column="del" javaType="java.lang.Integer"/>
            <columnOverride column="enum_attributes" javaType="java.util.List"
                            typeHandler="cn.zhengcaiyun.idata.develop.dal.JsonColumnHandler"/>
        </table>
        <table tableName="dev_foreign_key" mapperName="DevForeignKeyDao">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
            <columnOverride column="del" javaType="java.lang.Integer"/>
        </table>
    </context>
</generatorConfiguration>
