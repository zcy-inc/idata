<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="idata-map" targetRuntime="MyBatis3DynamicSql">
        <commentGenerator>
            <property name="suppressDate" value="true" />
            <property name="addRemarkComments" value="true" />
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://172.29.108.184:30106/idata"
                        userId="root" password="123456"/>

        <javaModelGenerator targetPackage="cn.zhengcaiyun.idata.map.dal.model" targetProject="src/main/java"/>
        <javaClientGenerator targetPackage="cn.zhengcaiyun.idata.map.dal.dao" targetProject="src/main/java"/>

<!--        <table tableName="map_view_count" mapperName="ViewCountDao">-->
<!--            <generatedKey column="id" sqlStatement="MySql" identity="true" />-->
<!--            <columnOverride column="del" javaType="java.lang.Integer"/>-->
<!--        </table>-->
<!--        -->

        <table tableName="map_user_favourite" mapperName="MapUserFavouriteDao">
            <generatedKey column="id" sqlStatement="MySql" identity="true" />
            <columnOverride column="del" javaType="java.lang.Integer"/>
        </table>

    </context>
</generatorConfiguration>
